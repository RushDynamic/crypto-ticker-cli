{
	"extends": ["airbnb-base"],
	"rules": {
		"no-console": "off",
		"import/newline-after-import": "warn",
		"comma-dangle": "off",
		"import/extensions": "off",
		"no-unused-vars": "warn",
		"no-loop-func": "off",
		"no-await-in-loop": "off",
		"indent": "off",
		"no-tabs": "off"
	},
	"plugins": ["prettier"]
}
